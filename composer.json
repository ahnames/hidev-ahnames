{
    "name": "ahnames/hidev-ahnames",
    "type": "yii2-extension",
    "description": "AHnames vendor configuration for HiDev",
    "keywords": [
        "ahnames",
        "hidev",
        "vendor",
        "config"
    ],
    "homepage": "https://github.com/ahnames/hidev-ahnames",
    "license": "proprietary",
    "support": {
        "email": "support@ahnames.com",
        "source": "https://github.com/ahnames/hidev-ahnames",
        "issues": "https://github.com/ahnames/hidev-ahnames/issues",
        "wiki": "https://github.com/ahnames/hidev-ahnames/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        }
    ],
    "require": {
        "hiqdev/composer-config-plugin": "<2.0"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ahnames\\hidev\\vendor\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "hidev": "src/config/hidev.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
