{
    "name": "ahnames/hidev-ahnames",
    "type": "yii2-extension",
    "description": "HiDev plugin for AHnames vendor configuration",
    "keywords": [
        "ahnames",
        "hidev",
        "vendor",
        "config"
    ],
    "homepage": "https://github.com/ahnames/hidev-ahnames",
    "license": "proprietary",
    "support": {
        "email": "support@ahnames.com",
        "source": "https://github.com/ahnames/hidev-ahnames",
        "issues": "https://github.com/ahnames/hidev-ahnames/issues",
        "wiki": "https://github.com/ahnames/hidev-ahnames/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        }
    ],
    "require": {
        "hiqdev/hidev-readme": "*@dev",
        "hiqdev/hidev-php-cs-fixer": "*@dev"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ahnames\\hidev\\ahnames\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "1.0": "1.0.x-dev"
        },
        "config-plugin": {
            "hidev": "config/hidev.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
